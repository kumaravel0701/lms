  </testcases>
</question>

<!-- Q38: Greatest of Two -->
<question type="coderunner">
  <name><text>Q38</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read two numbers. Print the greater one.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const [a, b] = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
console.log(a > b ? a : b);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>5 10</text></stdin><expected><text>10</text></expected></testcase>
    <testcase><stdin><text>20 15</text></stdin><expected><text>20</text></expected></testcase>
    <testcase><stdin><text>7 7</text></stdin><expected><text>7</text></expected></testcase>
    <testcase><stdin><text>-1 9</text></stdin><expected><text>9</text></expected></testcase>
  </testcases>
</question>

<!-- Q39: Greatest of Three -->
<question type="coderunner">
  <name><text>Q39</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read three numbers. Print the greatest.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const [a, b, c] = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
console.log(Math.max(a, b, c));
  ]]></answer>
  <testcases>
    <testcase><stdin><text>5 10 7</text></stdin><expected><text>10</text></expected></testcase>
    <testcase><stdin><text>20 15 25</text></stdin><expected><text>25</text></expected></testcase>
    <testcase><stdin><text>7 7 7</text></stdin><expected><text>7</text></expected></testcase>
    <testcase><stdin><text>-1 9 0</text></stdin><expected><text>9</text></expected></testcase>
  </testcases>
</question>



