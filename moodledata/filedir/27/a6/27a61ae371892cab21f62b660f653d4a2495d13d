<!-- Q30: Perimeter of Rectangle -->
<question type="coderunner">
  <name><text>Q30</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read length and width. Print the perimeter of rectangle.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const [l, w] = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
console.log(2 * (l + w));
  ]]></answer>
  <testcases>
    <testcase><stdin><text>5 10</text></stdin><expected><text>30</text></expected></testcase>
    <testcase><stdin><text>2 3</text></stdin><expected><text>10</text></expected></testcase>
