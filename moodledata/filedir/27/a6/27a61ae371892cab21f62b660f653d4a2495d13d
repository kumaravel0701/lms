<!-- Q33: Swap Two Numbers -->
<question type="coderunner">
  <name><text>Q33</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read two numbers and swap them. Print swapped values.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
let [a, b] = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
[a, b] = [b, a];
console.log(a + " " + b);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>5 10</text></stdin><expected><text>10 5</text></expected></testcase>
    <testcase><stdin><text>2 3</text></stdin><expected><text>3 2</text></expected></testcase>
    <testcase><stdin><text>7 7</text></stdin><expected><text>7 7</text></expected></testcase>
    <testcase><stdin><text>-1 9</text></stdin><expected><text>9 -1</text></expected></testcase>
  </testcases>
</question>

<!-- Q34: Simple Interest -->
<question type="coderunner">
  <name><text>Q34</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read P, R, T and print Simple Interest (P*R*T/100).</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const [p, r, t] = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
console.log((p * r * t) / 100);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>1000 5 2</text></stdin><expected><text>100</text></expected></testcase>
    <testcase><stdin><text>2000 10 1</text></stdin><expected><text>200</text></expected></testcase>
    <testcase><stdin><text>500 5 5</text></stdin><expected><text>125</text></expected></testcase>
    <testcase><stdin><text>1000 0 2</text></stdin><expected><text>0</text></expected></testcase>
  </testcases>
</question>

<!-- Q35: Convert Celsius to Fahrenheit -->
<question type="coderunner">
  <name><text>Q35</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read temperature in Celsius and convert to Fahrenheit using F = (C*9/5)+32.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const c = parseFloat(require('fs').readFileSync(0,'utf8').trim());
console.log((c * 9/5) + 32);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>0</text></stdin><expected><text>32</text></expected></testcase>
    <testcase><stdin><text>100</text></stdin><expected><text>212</text></expected></testcase>
    <testcase><stdin><text>-40</text></stdin><expected><text>-40</text></expected></testcase>
    <testcase><stdin><text>37</text></stdin><expected><text>98.6</text></expected></testcase>
  </testcases>
</question>

<!-- Q36: Convert Fahrenheit to Celsius -->
<question type="coderunner">
  <name><text>Q36</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read temperature in Fahrenheit and convert to Celsius using C = (F-32)*5/9.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const f = parseFloat(require('fs').readFileSync(0,'utf8').trim());
console.log((f - 32) * 5/9);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>32</text></stdin><expected><text>0</text></expected></testcase>
    <testcase><stdin><text>212</text></stdin><expected><text>100</text></expected></testcase>
    <testcase><stdin><text>-40</text></stdin><expected><text>-40</text></expected></testcase>
    <testcase><stdin><text>98.6</text></stdin><expected><text>37</text></expected></testcase>
  </testcases>
</question>

