<!-- Q13 -->
<question type="coderunner">
  <name><text>Q13</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read two numbers and print their sum.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const input = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
let a = input[0], b = input[1];
console.log(a + b);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>2 3</text></stdin><expected><text>5</text></expected></testcase>
    <testcase><stdin><text>10 20</text></stdin><expected><text>30</text></expected></testcase>
    <testcase><stdin><text>-5 5</text></stdin><expected><text>0</text></expected></testcase>
    <testcase><stdin><text>100 200</text></stdin><expected><text>300</text></expected></testcase>
  </testcases>
</question>

