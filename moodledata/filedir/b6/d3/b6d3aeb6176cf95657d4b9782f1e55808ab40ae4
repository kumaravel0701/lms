<!-- Q15 -->
<question type="coderunner">
  <name><text>Q15</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read two numbers and print "Difference is X".</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const [a, b] = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
console.log("Difference is " + (a - b));
  ]]></answer>
  <testcases>
    <testcase><stdin><text>10 5</text></stdin><expected><text>Difference is 5</text></expected></testcase>
    <testcase><stdin><text>20 30</text></stdin><expected><text>Difference is -10</text></expected></testcase>
    <testcase><stdin><text>50 50</text></stdin><expected><text>Difference is 0</text></expected></testcase>
    <testcase><stdin><text>-5 10</text></stdin><expected><text>Difference is -15</text></expected></testcase>
  </testcases>
