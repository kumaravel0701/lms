<!-- Q19 -->
<question type="coderunner">
  <name><text>Q19</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read two numbers and print "Quotient is X". (integer division)</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const [a, b] = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
console.log("Quotient is " + Math.floor(a / b));
  ]]></answer>
  <testcases>
    <testcase><stdin><text>10 2</text></stdin><expected><text>Quotient is 5</text></expected></testcase>
    <testcase><stdin><text>9 2</text></stdin><expected><text>Quotient is 4</text></expected></testcase>
    <testcase><stdin><text>7 3</text></stdin><expected><text>Quotient is 2</text></expected></testcase>
    <testcase><stdin><text>20 5</text></stdin><expected><text>Quotient is 4</text></expected></testcase>
  </testcases>
</question>

<!-- Q20 -->
<question type="coderunner">
  <name><text>Q20</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read three numbers and print their average.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const nums = require('fs').readFileSync(0,'utf8').trim().split(/\s+/).map(Number);
const avg = (nums[0] + nums[1] + nums[2]) / 3;
console.log(avg);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>3 3 3</text></stdin><expected><text>3</text></expected></testcase>
    <testcase><stdin><text>2 4 6</text></stdin><expected><text>4</text></expected></testcase>
    <testcase><stdin><text>10 20 30</text></stdin><expected><text>20</text></expected></testcase>
    <testcase><stdin><text>1 2 3</text></stdin><expected><text>2</text></expected></testcase>
  </testcases>
</question>

