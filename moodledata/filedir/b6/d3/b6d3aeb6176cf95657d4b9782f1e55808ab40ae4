<!-- Q18 -->
<question type="coderunner">
  <name><text>Q18</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Read a number and print its square.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[
const num = parseInt(require('fs').readFileSync(0,'utf8').trim());
console.log(num * num);
  ]]></answer>
  <testcases>
    <testcase><stdin><text>2</text></stdin><expected><text>4</text></expected></testcase>
    <testcase><stdin><text>5</text></stdin><expected><text>25</text></expected></testcase>
    <testcase><stdin><text>10</text></stdin><expected><text>100</text></expected></testcase>
