<!-- Q1 -->
<question type="coderunner">
  <name><text>Q1</text></name>
  <questiontext format="html">
    <text><![CDATA[ <p>Declare a mutable variable using <code>let</code> and assign it the value <code>"Hello"</code>. Print it.</p> ]]></text>
  </questiontext>
  <coderunnertype>nodejs</coderunnertype>
  <answer><![CDATA[ let greeting = "Hello"; console.log(greeting); ]]></answer>
  <testcases>
    <testcase><expected><text>Hello</text></expected></testcase>
  </testcases>
</question>

